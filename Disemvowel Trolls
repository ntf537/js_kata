function disemvowel(str) {
  let newStr = "";
  for (let i = 0; i < str.length; i++)
    {
      if(!((str[i] == 'a') || (str[i] == 'e') || (str[i] == 'i') || (str[i] == 'o') || (str[i] == 'u') || (str[i] == 'o') ||
        (str[i] == 'A') || (str[i] == 'E') || (str[i] == 'I') || (str[i] == 'O') || (str[i] == 'U')))
        {
          newStr += str[i];
        }
    }
  return newStr;
}
